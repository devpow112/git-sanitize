{
  "name": "git-sanitize",
  "version": "1.0.0",
  "description": "Git custom command that will do a full \"sanitize\" of a branch",
  "main": "dist/index.js",
  "bin": {
    "git-sanitize": "./bin/git-sanitize"
  },
  "scripts": {
    "build": "babel --out-dir dist src",
    "lint": "npm run -s lint:git && npm run -s lint:js && npm run -s lint:md",
    "lint:js": "eslint .",
    "lint:md": "markdownlint .",
    "lint:git": "commitlint --from origin/main --to HEAD",
    "format": "npm run -s format:js && npm run -s format:md",
    "format:js": "npm run lint:js -s -- --fix",
    "format:md": "npm run lint:md -s -- --fix",
    "test": "npm run lint -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devpow112/git-sanitize.git"
  },
  "keywords": [
    "git",
    "sanitize"
  ],
  "author": "Devon Powell <devon.f.powell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devpow112/git-sanitize/issues"
  },
  "homepage": "https://github.com/devpow112/git-sanitize#readme",
  "files": [
    "bin/*",
    "dist/**/*.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "command-exists": "^1.2.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/git": "^9.0.0",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "markdownlint-cli": "^0.26.0",
    "semantic-release": "^17.3.7"
  }
}
